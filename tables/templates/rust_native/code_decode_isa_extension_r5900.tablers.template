    pub(crate) const fn decode_isa_extension_r5900_mmi(
        word: u32,
        mut mandatory_bits: EncodedFieldMask,
        flags: &DecodingFlags,
        isa_version: IsaVersion,
    ) -> Self {
        let mask = EncodedFieldMask::function;
        let opcode;
        let opcode_category = OpcodeCategory::R5900_MMI;

        mandatory_bits = mandatory_bits.union(mask.mask_value(word));

        match mask.get_shifted(word) {
#include "instr_id/r5900/r5900_mmi.inc"

            0x08 => return Self::decode_isa_extension_r5900_mmi_0(word, mandatory_bits, flags, isa_version),
            0x09 => return Self::decode_isa_extension_r5900_mmi_2(word, mandatory_bits, flags, isa_version),
            0x28 => return Self::decode_isa_extension_r5900_mmi_1(word, mandatory_bits, flags, isa_version),
            0x29 => return Self::decode_isa_extension_r5900_mmi_3(word, mandatory_bits, flags, isa_version),

            0x30 => return Self::decode_isa_extension_r5900_mmi_pmfhl(word, mandatory_bits, flags, isa_version),
            0x31 => return Self::decode_isa_extension_r5900_mmi_pmthl(word, mandatory_bits, flags, isa_version),

            _ => opcode = Opcode::ALL_INVALID,
        }
        Self {
            opcode,
            opcode_category,
            mandatory_bits,
        }
    }

